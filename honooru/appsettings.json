{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "honooru": "Trace",
            "honooru.Services.IqdbClient": "Information",
            "honooru.Services.Hosted.HealthServiceChecker": "Information",
            "honooru.Services.ApiKeyAuthHandler": "Information"
        }
    },
    "AllowedHosts": "*",
    "Discord": {
        "Enabled": false
    },
    "Services": {
        "LogoutBuffer": true
    },
    "Http": {
        "SearchEngineUserAgents": [
            "Yandex",
            "Googlebot",
            "Bingbot"
        ],
        "RealIpHeader": "X-Connected-IP"
    },
    "ConnectionStrings": {
        "main": "Host=psql;Username=postgres;Password=password;Port=5432;Include Error Detail=true;ApplicationName=honooru;Timezone=UTC"
    },
    "Storage": {
        "RootDirectory": "/plap"
    },
    "Iqdb": {
        "Host": "http://iqdb:5588"
    },
    "Extractor": {
        "DistributeYoutubeJobs": false
    }
}
