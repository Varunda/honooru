{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "honooru": "Trace"
        }
    },
    "AllowedHosts": "*",
    "Discord": {
        "Enabled": false
    },
    "Services": {
        "LogoutBuffer": true
    },
    "Http": {
        "SearchEngineUserAgents": [
            "Yandex",
            "Googlebot",
            "Bingbot"
        ],
        "RealIpHeader": "X-Connected-IP"
    },
    "ConnectionStrings": {
        "main": "Host=localhost;Username=postgres;Password=password;Database=annuls;Port=5432;Include Error Detail=true;ApplicationName=honooru;Timezone=UTC"
    },
    "Storage": {
        "RootDirectory": "E:/annuls"
    },
    "Iqdb": {
        "Host": "http://localhost:5588"
    }
}
